<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
https://raw.githubusercontent.com/Gethe/wow-ui-source/live/Interface/AddOns/Blizzard_SharedXML/UI.xsd">
	<Script file="StatusBarTemplate.lua"/>
		
	<Frame name="HDH_AT_MultiStatusBarTemplate" mixin="HDH_AT_MultiStatusBarTemplateMixin" virtual="true">
		<!-- 입력된 값에 따라서 모든걸 조정할 것이기 때문에 코드로 내부 구조를 생성함 -->
	</Frame>

	<StatusBar name="HDH_AT_StatusBarTemplate" mixin="HDH_AT_StatusBarTemplateMixin" virtual="true">
		<Layers>
			<Layer level="OVERLAY">
				<Texture parentKey="Spark" alphaMode="ADD">
				</Texture>
			</Layer>
		</Layers>
		<Scripts>
			<OnValueChanged function="HDH_AT_StatusBar_OnValueChanged"/>
		</Scripts>
	</StatusBar>
    
</Ui>