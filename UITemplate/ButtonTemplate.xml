						<Anchor point="BOTTOMRIGHT" x="-10" y="0"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Button name="$parentConfig" parentKey="ConfigBtn" hidden="true">
				<Size x="20" y="20"/>
				<Anchors>
					<Anchor point="LEFT" relativePoint="LEFT" x="5" y="0" />
				</Anchors>
				<NormalTexture file="Interface\AddOns\HDH_AuraTracker\Texture\config" alphaMode="ADD">
					<Color r="0.25" g="0.25" b="0.25" a="0.65"/>
				</NormalTexture>
				<HighlightTexture file="Interface\AddOns\HDH_AuraTracker\Texture\config" alphaMode="ADD">
					<Color r="1" g="0.8" b="0.1" a="0.65"/>
				</HighlightTexture>

				<PushedTexture file="Interface\AddOns\HDH_AuraTracker\Texture\config">
					<Color r="1" g="0.8" b="0.1" a="0.65"/>
					<TexCoords left="-0.1" right="1.1" top="-0.1" bottom="1.1" />
				</PushedTexture>
				
				<Scripts>
					<OnLeave>
						if not self:GetParent():IsMouseOver() then
							self:Hide()
						end
					</OnLeave>
				</Scripts>
			</Button>
		</Frames>

		<Scripts>
			<OnEnter>
				if not self.isAddBtn then
					self.ConfigBtn:Show()
				end
			</OnEnter>
			<OnLeave>
				if not self.ConfigBtn:IsMouseOver() then
					self.ConfigBtn:Hide()
				end
			</OnLeave>
		</Scripts>

		<ButtonText name="$parentText" parentKey="Text" maxLines="1" justifyH="CENTER">
			<Anchors>
				<Anchor point="TOPLEFT" x="5" y="0"/>
				<Anchor point="BOTTOMRIGHT" x="-5" y="0"/>
			</Anchors>
