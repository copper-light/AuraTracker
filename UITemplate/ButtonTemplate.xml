	<Button name="HDH_AT_PrevNextButtonTemplate" virtual="true" enableMouse="false">	
		<Size><AbsDimension x="90" y="22" /></Size>
		<Layers> 
			<Layer level="ARTWORK">
				<FontString name="$parentTextValue" parentKey="Value" inherits="Font_Yellow_S" justifyH="CENTER" text="1">
					<Anchors>
						<Anchor point="CENTER" relativePoint="CENTER" x="0" y="0" />
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<Button parentKey="BtnPrev" inherits="HDH_AT_TextureButtonTemplate">
				<Size><AbsDimension x="22" y="22" /></Size>
				<Layers>
					<Layer level="ARTWORK" textureSubLevel="2">
						<Texture parentKey="Texture" file="Interface/AddOns/HDH_AuraTracker/Texture/Left">
							<TexCoords left="0" right="1" top="0" bottom="1" />
							<Size x="8" y="10" />
							<Anchors>
								<Anchor point="CENTER" x="0" y="0"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="ARTWORK" textureSubLevel="1">
						<Texture file="Interface/AddOns/HDH_AuraTracker/Texture/Left">
							<TexCoords left="0" right="1" top="0" bottom="1" />
							<Size x="8" y="10" />
							<Color r="0" g="0" b="0" a="0.5"/>
							<Anchors>
								<Anchor point="CENTER" relativeKey="$parent.Texture" relativePoint="CENTER" x="1" y="-1"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Anchors>
					<Anchor point="LEFT" relativePoint="LEFT" x="0" y="0"/>
				</Anchors>
			</Button>

			<Button parentKey="BtnNext" inherits="HDH_AT_TextureButtonTemplate">
				<Size><AbsDimension x="22" y="22" /></Size>
				<Layers>
					<Layer level="ARTWORK" textureSubLevel="2">
						<Texture parentKey="Texture" file="Interface/AddOns/HDH_AuraTracker/Texture/Left">
							<TexCoords left="1" right="0" top="0" bottom="1" />
							<Size x="8" y="10" />
							<Anchors>
								<Anchor point="CENTER" x="0" y="0"/>
							</Anchors>
						</Texture>
					</Layer>
					<Layer level="ARTWORK" textureSubLevel="1">
						<Texture file="Interface/AddOns/HDH_AuraTracker/Texture/Left">
							<TexCoords left="1" right="0" top="0" bottom="1" />
							<Size x="8" y="10" />
							<Color r="0" g="0" b="0" a="0.5"/>
							<Anchors>
								<Anchor point="CENTER" relativeKey="$parent.Texture" relativePoint="CENTER" x="1" y="-1"/>
							</Anchors>
						</Texture>
					</Layer>
				</Layers>
				<Anchors>
					<Anchor point="RIGHT" relativePoint="RIGHT" x="0" y="0"/>
				</Anchors>
			</Button>
		</Frames>
		<Scripts>
			<OnEnable>
				self.BtnPrev:Show()
				self.BtnNext:Show()
			</OnEnable>
			<OnDisable>
				self.BtnPrev:Hide()
				self.BtnNext:Hide()
			</OnDisable>
		</Scripts>
	</Button>

	<Button name="HDH_AT_TextureButtonTemplate" inherits="HDH_AT_ButtonTemplate" virtual="true" >
		<Size><AbsDimension x="22" y="22" /></Size>
		<Scripts>
			<OnMouseDown>
				if self.Texture then
					self.Texture:SetPoint("CENTER", 1, -1)
				end
			</OnMouseDown>
			<OnMouseUp>
				if self.Texture then
					self.Texture:SetPoint("CENTER", 0, 0)
				end
			</OnMouseUp>
		</Scripts>
	</Button>

